{
  "name": "later-mate",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start -c",
    "expo-doctor": "npx expo install --check && npx expo-doctor",
    "devtools": "run react-devtools",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "ts:check": "yarn tsc --noemit",
    "start-dev-client": "npx expo start --dev-client",
    "build-ios-locally": "npx expo run:ios --configuration Release",
    "build-android-locally": "npx expo run:android --variant release",
    "format:check": "npx prettier . --check",
    "format": "npx prettier . --write",
    "lint:check": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "jest --watchAll",
    "lint": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
    "clean:node_modules": "rm -rf node_modules/",
    "eas:list": "eas secret:list",
    "submit:staging:ios": "eas submit -p ios --latest --profile preview",
    "submit:ios": "eas submit -p ios --latest",
    "submit:staging:android": "eas submit -p android --latest --profile preview",
    "submit:android": "eas submit -p android --latest",
    "build:ios": "eas build  --platform ios",
    "build:android": "eas build --platform android",
    "build:staging:android": "eas build --profile preview --platform android",
    "build:staging:ios": "eas build --profile preview --platform ios",
    "build:dev:ios": "eas build --profile development --platform ios",
    "build:dev:android": "eas build --profile development --platform android",
    "create:eas-secret": "eas secret:push --scope project --env-file .env --force"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-native-community/datetimepicker": "7.2.0",
    "@react-native-community/netinfo": "9.3.10",
    "@react-navigation/native": "^6.0.2",
    "@shopify/flash-list": "1.4.3",
    "@tanstack/react-query": "^4.35.7",
    "axios": "^1.5.1",
    "dotenv": "^16.3.1",
    "expo": "~49.0.13",
    "expo-apple-authentication": "~6.1.0",
    "expo-auth-session": "~5.0.2",
    "expo-clipboard": "~4.3.1",
    "expo-device": "~5.4.0",
    "expo-font": "~11.4.0",
    "expo-linear-gradient": "~12.3.0",
    "expo-linking": "~5.0.2",
    "expo-notifications": "~0.20.1",
    "expo-router": "^2.0.0",
    "expo-secure-store": "~12.3.1",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "expo-system-ui": "~2.4.0",
    "expo-web-browser": "~12.3.2",
    "formik": "^2.4.5",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.5",
    "react-native-actions-sheet": "^0.9.0-alpha.24",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-loading-spinner-overlay": "^3.0.1",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-svg": "13.9.0",
    "react-native-svg-transformer": "^1.1.0",
    "react-native-web": "~0.19.6",
    "react-native-zohosalesiq-mobilisten": "^6.1.0",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-native": "^12.1.2",
    "@types/jest": "^29.5.2",
    "@types/react": "~18.2.14",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "^29.5.0",
    "jest-expo": "^48.0.2",
    "prettier": "^2.8.8",
    "react-devtools": "^4.27.8",
    "react-test-renderer": "^18.2.0",
    "tailwindcss": "3.3.2",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.3"
  },
  "overrides": {
    "react-refresh": "~0.14.0"
  },
  "resolutions": {
    "react-refresh": "~0.14.0"
  },
  "private": true
}
